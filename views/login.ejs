<html lang="en">
<head>
  <title>Test Login Page</title>
</head>
<body>
  <div class="container">
      <h4>Login</h4>
      <form method="POST" action="/login">
        <div class="form-group">
          <label for="email-input-field">Email address</label>
          <input type="email" name="email" class="form-control" id="email-input-field"  placeholder="Enter email" required>
        </div>
        <div class="form-group">
          <label for="password-field">Password</label>
          <input type="password" name="password" class="form-control" id="password-field" placeholder="Password" required>
        </div>
        <button type="submit" class="btn btn-primary">Login</button>
      </form>
    </div>
</body>
</html>